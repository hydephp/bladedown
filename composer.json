{
  "name": "hyde/bladedown",
  "description": "Enter a description for your package here",
  "keywords": [
    "hydephp",
    "package",
    "extension"
  ],
  "homepage": "https://github.com/hydephp/bladedown",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Caen De Silva",
      "email": "caen@desilva.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "hyde/framework": "^1.0",
    "illuminate/support": "^10.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Hyde\\Bladedown\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hyde\\Bladedown\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hyde\\Bladedown\\BladedownServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}